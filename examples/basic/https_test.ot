use http

fn main():
    println("Testing HTTPS...")
    let url = "https://worldtimeapi.org/api/timezone/Etc/UTC"
    let status = http.head(url)
    println(f"HTTPS HEAD status: {status}")
    
    let body = http.get(url)
    if body == "":
        println("HTTPS GET returned empty body!")
    else:
        println(f"HTTPS GET body length: {len(body)}")
        println("Body:")
        println(body)
